{
  "name": "mgnrega-dashboard-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:seed": "npx ts-node --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.0.0",
    "helmet": "^4.6.0",
    "ioredis": "^4.28.5",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "prisma": "^4.0.0",
    "prom-client": "^14.0.0",
    "winston": "^3.3.3",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^4.28.10",
    "@types/morgan": "^1.9.10",
    "@types/node-cron": "^3.0.11",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
